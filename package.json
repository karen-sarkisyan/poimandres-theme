{
  "name": "pmndrs-rusty",
  "icon": "assets/pmndrs.png",
  "displayName": "pmndrs-rusty",
  "description": "pmndrs-rusty dark theme",
  "version": "0.3.7",
  "keywords": [
    "poimandres",
    "pmndrs",
    "vsc",
    "dark",
    "theme"
  ],
  "files": [
    "index.js"
  ],
  "scripts": {
    "dev": "node src/watcher"
  },
  "author": "Paul Henschel",
  "publisher": "pmndrs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drcmda/poimandres-theme/issues"
  },
  "homepage": "https://github.com/drcmda/poimandres-theme#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drcmda/poimandres-theme.git"
  },
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "pmndrs-rusty",
        "uiTheme": "vs-dark",
        "path": "./themes/pmndrs-rusty-color-theme.json"
      },
      {
        "label": "pmndrs-rusty-noitalics",
        "uiTheme": "vs-dark",
        "path": "./themes/pmndrs-rusty-color-theme-noitalics.json"
      },
      {
        "label": "pmndrs-rusty-storm",
        "uiTheme": "vs-dark",
        "path": "./themes/pmndrs-rusty-color-theme-storm.json"
      },
      {
        "label": "pmndrs-rusty-noitalics-storm",
        "uiTheme": "vs-dark",
        "path": "./themes/pmndrs-rusty-color-theme-noitalics-storm.json"
      }
    ]
  },
  "devDependencies": {
    "filewatcher": "^3.0.1",
    "prettier": "^2.2.1",
    "sharp": "^0.28.0"
